<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    
    <class name="com.samaritan.opus.model.Artiste" table="artiste" schema="opus">
        
        <id name="id" column="id" type="java.lang.Integer">
            <generator class="identity"/>
        </id>
        
        <property name="firstName" column="first_name"/>
        
        <property name="lastName" column="last_name"/>
            
        <property name="bankAccountNumber" column="bank_acc_number"/>
           
        <property name="stageName" column="stage_name"/>
            
        <property name="profilePicture" column="profile_picture"/>
        
        <property name="numberOfSongs" column="number_of_songs"/>
        
        <property name="numberOfAlbums" column="number_of_albums"/>
            
    </class>
    
</hibernate-mapping>
